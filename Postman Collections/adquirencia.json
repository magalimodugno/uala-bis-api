				{
					"name": "Cognito User Token - AR",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.sendRequest({",
									"           url: \"https://cognito-idp.us-east-1.amazonaws.com/\",",
									"           method: 'POST',",
									"           header: {",
									"                    'X-Amz-Target': 'AWSCognitoIdentityProviderService.InitiateAuth',",
									"                    'Content-Type': 'application/x-amz-json-1.1'",
									"                   },",
									"            body: {",
									"                   mode: 'raw',",
									"                   raw: JSON.stringify({",
									"                        \"AuthParameters\": {",
									"                        \"USERNAME\": pm.environment.get(\"username\"),",
									"                        \"PASSWORD\": pm.environment.get(\"password\")",
									"                        },",
									"                        \"AuthFlow\": \"USER_PASSWORD_AUTH\",",
									"                        \"ClientId\": pm.environment.get(\"cognito_client_id\")",
									"                    }),",
									"            options: { ",
									"                raw: { ",
									"                        language: 'json' ",
									"                    } ",
									"                }",
									"            }",
									"}, function (error, response) {",
									"    //Open CONSOLE if u want to see the final token",
									"    console.log(\"Reponse \", response.json())",
									"    console.log(\"Reponse status \", response.code, \"with message: \", response.status)",
									"    console.log(\"TOKEN: \", response.json().AuthenticationResult.IdToken);",
									"    pm.environment.set(\"token\", response.json().AuthenticationResult.IdToken);",
									"});"
								],
								"type": "text/javascript",
								"id": "a0666570-92c8-47be-b0b9-ba61cc5b20f0"
							}
						}
					],
					"id": "08126fa9-c591-4d43-bcfb-40add2d3f962",
